# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Vercel-compatible Supabase variables
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# Admin emails (comma-separated)
ADMIN_EMAILS=admin1@example.com,admin2@example.com

# Admin debug mode (set to true to bypass authentication in development)
ADMIN_DEBUG_MODE=false

# Site configuration
SITE_NAME=Dutch Seed Supply
SITE_DOMAIN=dutchseedsupply.com
SITE_DESCRIPTION=Your site description

# Revalidation
REVALIDATION_SECRET=your-revalidation-secret
NEXT_PUBLIC_REVALIDATION_SECRET=your-revalidation-secret
VERCEL_DEPLOYMENT_URL=https://dutchseedsupply.com

# Authentication
AUTH0_SECRET=your-auth0-secret
AUTH0_BASE_URL=your-auth0-base-url
AUTH0_ISSUER_BASE_URL=your-auth0-issuer-base-url
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# PostgreSQL configuration (if using direct connection)
POSTGRES_HOST=your-postgres-host
POSTGRES_USER=your-postgres-user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DATABASE=your-postgres-database
POSTGRES_URL=your-postgres-url
POSTGRES_URL_NON_POOLING=your-postgres-url-non-pooling
POSTGRES_PRISMA_URL=your-postgres-prisma-url
